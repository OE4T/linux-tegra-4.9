#include "tegra210-jetson-e-base-p2595-0000-a00.dts"
#include "tegra210-platforms/tegra210-p2530-common.dtsi"

/ {
	model = "jetson_e";
	compatible = "nvidia,jetson-e", "nvidia,tegra210";
	nvidia,dtsfilename = __FILE__;

	#address-cells = <2>;
	#size-cells = <2>;

	chosen {
		bootargs = "earlycon=uart8250,mmio32,0x70006000 androidboot.hardware=jetson_e";
	};

	firmware {
		android {
			compatible = "android,firmware";
			hardware = "jetson_e";
		};
	};

	udc@7d000000 {
		#extcon-cells = <1>;
		status = "disabled";
		dt-override-status-odm-data = <0x01000000 0x00000000>;
	};

        otg@7d000000 {
		#extcon-cells = <1>;
		status = "disabled";
		dt-override-status-odm-data = <0x01000000 0x00000000>;
	};

	xudc@700d0000 {
		#extcon-cells = <1>;
		status = "disabled";
		dt-override-status-odm-data = <0x01000000 0x01000000>;
	};

	xotg {
		#extcon-cells = <1>;
		status = "disabled";
		dt-override-status-odm-data = <0x01000000 0x01000000>;
	};

};
